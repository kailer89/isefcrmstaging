<h1>Territorios</h1>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Nombre</th>
      <th>Fecha de Creaci√≥n</th>
      <th>Acciones</th>
    </tr>
  </thead>
  <tbody>
    <% @territorios.each do |territorio| %>
      <tr>
        <td><%= territorio.id %></td>
        <td><%= link_to territorio.nombre, territorio_path(territorio) %></td>
        <td><%= territorio.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_territorio_path(territorio), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', territorio_path(territorio), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_territorio_path, :class => 'btn btn-primary' %>
