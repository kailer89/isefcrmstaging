<h1>Employmentships</h1>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>User</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @employmentships.each do |employmentship| %>
      <tr>
        <td><%= employmentship.id %></td>
        <td><%= link_to employmentship.user_id, employmentship_path(employmentship) %></td>
        <td><%= employmentship.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_employmentship_path(employmentship), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', employmentship_path(employmentship), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_employmentship_path, :class => 'btn btn-primary' %>
