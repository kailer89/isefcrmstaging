<h1>Preparatoria O Universidad De Origens</h1>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Valor</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @preparatoria_o_universidad_de_origens.each do |preparatoria_o_universidad_de_origen| %>
      <tr>
        <td><%= preparatoria_o_universidad_de_origen.id %></td>
        <td><%= link_to preparatoria_o_universidad_de_origen.valor, preparatoria_o_universidad_de_origen_path(preparatoria_o_universidad_de_origen) %></td>
        <td><%= preparatoria_o_universidad_de_origen.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_preparatoria_o_universidad_de_origen_path(preparatoria_o_universidad_de_origen), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', preparatoria_o_universidad_de_origen_path(preparatoria_o_universidad_de_origen), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_preparatoria_o_universidad_de_origen_path, :class => 'btn btn-primary' %>
