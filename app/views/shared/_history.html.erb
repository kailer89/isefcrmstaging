<%	historiales =History.where(:model_name=>model_name ,:model_id=>model_id.to_s)%>    
    <div class="tab-pane fade" id="history<%=model_id%>">
      <fieldset>
        <!--<legend><%= controller.action_name.capitalize %> Prospecto</legend>-->
        <div  class="well">        
			<h3>Historial de acciones</h3>
			    <table class="table table-striped table-bordered table-condensed table-bordered table-condensed">
			      <thead>
			        <tr>
			          <th>Accion</th>
			          <th>Usuario</th>
			          <th>Creado en </th>
			        </tr>
			      </thead>
			      <tbody>
			        <% historiales.each do |historial| %>
			          <tr>
			            <td><%= historial.action %></td>
			            <%
			            if User.where(:id=>historial.user_id).exists?
			            %>
			            	<td><%= User.where(:id=>historial.user_id).first.username %></td>
			            <%else%>
			            <td>Usuario no existe (<%=historial.user_id%>)</td>
			            <%end%>
			            
			            <td><%= historial.created_at %></td>
			          </tr>
			        <% end %>
			      </tbody>
			    </table>
        </div>
      </fieldset>

    </div>
