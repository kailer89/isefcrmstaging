  <%
  #render a partial for any tweets
	tweets =Tweet.where(:model_name=>request.path_parameters[:controller] ,:model_id=>model_id)
  %>
  <div class="well">
    <h3>Tweets</h3>
     <table class="table table-striped table-bordered table-condensed">
      <thead>
        <tr>
          <th>ID</th>
          <th>Nombre</th>
          <th>Creado en </th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody>
        <% tweets.each do |tweet| %>
          <tr>
            <td><%= link_to tweet.id, edit_tweet_path(tweet) %></td>
            <td><%= link_to tweet.tweet, edit_tweet_path(tweet) %></td>
            <td><%= link_to tweet.created_at, edit_tweet_path(tweet) %></td>
            <td>
              <%= link_to 'Edit', edit_tweet_path(tweet), :class => 'btn btn-mini' %>
              <%= link_to 'Destroy', tweet_path(tweet), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <%= link_to 'Nuevo Tweet', new_tweet_path(:model_name=>request.path_parameters[:controller],:model_id=>model_id), :class => 'btn btn-primary' %>
  </div>