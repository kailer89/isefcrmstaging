<span class="fields" data-object-name="<%= f.object_name %>">
  <%= f.attribute_fields do |a| %>
    <%= a.attribute_select associations: [:solicitantes,:solicitantes_prospectos,:solicitantes_prospectos_nacionalidad, :solicitantes_prospectos_programa,:solicitantes_prospectos_user,:solicitantes_prospectos_sede,:solicitantes_prospectos_grupo,:solicitantes_prospectos_direccions,:solicitantes_prospectos_interes_basicos,:solicitantes_prospectos_interes_academicos,:solicitantes_prospectos_plan_de_descuentos] %>
  <% end %>
  <%= f.predicate_select %>
  <%= f.value_fields do |v| %>
    <%= v.text_field :value %>
  <% end %>
  <%= button_to_remove_fields "remove", f %>
</span>
<br/>