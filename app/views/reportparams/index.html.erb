<h1>Reportparams</h1>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Nombre</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @reportparams.each do |reportparam| %>
      <tr>
        <td><%= reportparam.id %></td>
        <td><%= link_to reportparam.nombre, reportparam_path(reportparam) %></td>
        <td><%= reportparam.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_reportparam_path(reportparam), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', reportparam_path(reportparam), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_reportparam_path, :class => 'btn btn-primary' %>
