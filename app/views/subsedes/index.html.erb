<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Nombre</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @subsedes.each do |subsede| %>
      <tr>
        <td><%= subsede.id %></td>
        <td><%= link_to subsede.nombre, subsede_path(subsede) %></td>
        <td><%= subsede.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_subsede_path(subsede), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', subsede_path(subsede), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_subsede_path, :class => 'btn btn-primary' %>
