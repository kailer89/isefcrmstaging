<h1>Campa√±a Social Por Correo</h1>
<table class="table table-striped table-bordered table-condensed table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Nombre</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <%if @campana_socials != nil %>
    <% @campana_socials.each do |campana_social|%>
      <tr>
        <td><%= campana_social.id %></td>
        <td><%= link_to campana_social.nombre, campana_social_path(campana_social) %></td>
        <td><%= campana_social.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_campana_social_path(campana_social), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', campana_social_path(campana_social), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
    <% end %>
  </tbody>
</table>
 <%if @campana_socials != nil %>
<%= will_paginate @campana_socials %>
<% end %>
<%= link_to 'New', new_campana_social_path(:tipo=>params[:tipo]), :class => 'btn btn-primary' %>
