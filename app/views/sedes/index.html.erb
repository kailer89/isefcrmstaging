<h1>Sedes</h1>
<div class="well">
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Nombre</th>
      <th>Fecha de Creaci√≥n</th>
      <th>Acciones</th>
    </tr>
  </thead>
  <tbody>
    <% @sedes.each do |sede| %>
      <tr>
        <td><%= sede.id %></td>
        <td><%= link_to sede.nombre, sede_path(sede) %></td>
        <td><%= sede.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_sede_path(sede), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', sede_path(sede), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<%= link_to 'New', new_sede_path, :class => 'btn btn-primary' %>
