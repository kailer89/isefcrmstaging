<h1>Nacionalidads</h1>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Nacionalidad</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @nacionalidads.each do |nacionalidad| %>
      <tr>
        <td><%= nacionalidad.id %></td>
        <td><%= link_to nacionalidad.nacionalidad, nacionalidad_path(nacionalidad) %></td>
        <td><%= nacionalidad.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_nacionalidad_path(nacionalidad), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', nacionalidad_path(nacionalidad), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_nacionalidad_path, :class => 'btn btn-primary' %>
