<h1>Preguntas Frecuentes</h1>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Ambito de la pregunta</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @pregunta.each do |preguntum| %>
      <tr>
        <td><%= preguntum.id %></td>
        <td><%= link_to preguntum.ambito_de_la_pregunta, preguntum_path(preguntum) %></td>
        <td><%= preguntum.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_preguntum_path(preguntum), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', preguntum_path(preguntum), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_preguntum_path, :class => 'btn btn-primary' %>
