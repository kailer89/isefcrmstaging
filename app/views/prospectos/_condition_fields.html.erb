<span class="fields" data-object-name="<%= f.object_name %>">
  <%= f.attribute_fields do |a| %>
    <%= a.attribute_select associations: [:nacionalidad, :programa,:user,:sede,:grupo,:direccions,:interes_basicos,:interes_academicos,:plan_de_descuentos] %>
  <% end %>
  <%= f.predicate_select %>
  <%= f.value_fields do |v| %>
    <%= v.text_field :value %>
  <% end %>
  <%= button_to_remove_fields "remove", f %>
</span>
<br/>