<h1>Equipos de Promocion</h1>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Nombre</th>
      <th>Fecha de Creaci√≥n</th>
      <th>Acciones</th>
    </tr>
  </thead>
  <tbody>
    <% @equipo_promocions.each do |equipo_promocion| %>
      <tr>
        <td><%= equipo_promocion.id %></td>
        <td><%= link_to equipo_promocion.nombre, equipo_promocion_path(equipo_promocion) %></td>
        <td><%= equipo_promocion.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_equipo_promocion_path(equipo_promocion), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', equipo_promocion_path(equipo_promocion), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_equipo_promocion_path, :class => 'btn btn-primary' %>
