<h1>Imports</h1>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Filename</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @imports.each do |import| %>
      <tr>
        <td><%= import.id %></td>
        <td><%= link_to import.filename, import_path(import) %></td>
        <td><%= import.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_import_path(import), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', import_path(import), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_import_path, :class => 'btn btn-primary' %>
