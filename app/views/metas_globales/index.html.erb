<%- model_class = MetasGlobale.new.class -%>
<h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:periodo_anual) %></th>
      <th><%= model_class.human_attribute_name(:sede_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @metas_globales.each do |metas_globale| %>
      <tr>
        <td><%= link_to metas_globale.id, metas_globale_path(metas_globale) %></td>
        <td><%= metas_globale.periodo_anual %></td>
        <td><%
         elPrograma = Sede.where(:id=>metas_globale.sede_id).first
    %>
    <%=elPrograma.nombre%></td>
        <td><%=l metas_globale.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_metas_globale_path(metas_globale), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      metas_globale_path(metas_globale),
                      :method => :delete,
                      :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_metas_globale_path,
            :class => 'btn btn-primary' %>
<br/>
<br/>
<br/>