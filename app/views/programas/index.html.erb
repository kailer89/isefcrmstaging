<h1>Programas</h1>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Nivel</th>
      <th>Programa</th>
      <th>Fecha de Creaci√≥n</th>
      <th>Acciones</th>
    </tr>
  </thead>
  <tbody>
    <% @programas.each do |programa| %>
      <tr>
        <td><%= programa.id %></td>
        <td><%= link_to programa.nivel, programa_path(programa) %></td>
        <td><%= link_to programa.programa, programa_path(programa) %></td>
        <td><%= programa.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_programa_path(programa), :class => 'btn btn-mini' %>
          <%= link_to 'Borrar', programa_path(programa), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_programa_path, :class => 'btn btn-primary' %>
