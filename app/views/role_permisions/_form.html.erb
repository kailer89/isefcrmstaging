<%= simple_form_for(@role_permision) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :objeto %>
    <%= f.input :permiso %>
    <%= f.input :user_id %>
    <%= f.input :role_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
