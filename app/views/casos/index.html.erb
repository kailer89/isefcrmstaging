<h1>Casos</h1>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Caso</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @casos.each do |caso| %>
      <tr>
        <td><%= caso.id %></td>
        <td><%= link_to caso.caso, caso_path(caso) %></td>
        <td><%= caso.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_caso_path(caso), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', caso_path(caso), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_caso_path, :class => 'btn btn-primary' %>
