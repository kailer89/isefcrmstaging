<h1>Post</h1>

<p>
  <b>Mensaje</b><br>
  <%= @post.mensaje %>
</p>

<p>
  <b>Whensaved</b><br>
  <%= @post.whensaved %>
</p>

<p>
  <b>Created by</b><br>
  <%= @post.created_by %>
</p>

<p>
  <b>User</b><br>
  <%= @post.user_id %>
</p>

<p>
  <b>Sede</b><br>
  <%= @post.sede_id %>
</p>

<div class="form-actions">
  <%= link_to 'Back', posts_path, :class => 'btn'  %>
  <%= link_to 'Edit', edit_post_path(@post), :class => 'btn' %>
  <%= link_to 'Delete', post_path(@post), :method => 'delete', :confirm => 'Are you sure?', :class => 'btn btn-danger' %>
</div>
