<h1>Tipo De Admisions</h1>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Valor</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @tipo_de_admisions.each do |tipo_de_admision| %>
      <tr>
        <td><%= tipo_de_admision.id %></td>
        <td><%= link_to tipo_de_admision.valor, tipo_de_admision_path(tipo_de_admision) %></td>
        <td><%= tipo_de_admision.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_tipo_de_admision_path(tipo_de_admision), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', tipo_de_admision_path(tipo_de_admision), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_tipo_de_admision_path, :class => 'btn btn-primary' %>
