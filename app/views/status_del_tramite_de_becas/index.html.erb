<h1>Status Del Tramite De Becas</h1>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Valor</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @status_del_tramite_de_becas.each do |status_del_tramite_de_beca| %>
      <tr>
        <td><%= status_del_tramite_de_beca.id %></td>
        <td><%= link_to status_del_tramite_de_beca.valor, status_del_tramite_de_beca_path(status_del_tramite_de_beca) %></td>
        <td><%= status_del_tramite_de_beca.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_status_del_tramite_de_beca_path(status_del_tramite_de_beca), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', status_del_tramite_de_beca_path(status_del_tramite_de_beca), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_status_del_tramite_de_beca_path, :class => 'btn btn-primary' %>
