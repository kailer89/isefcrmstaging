<h1>Reportsortbys</h1>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Fieldname</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @reportsortbies.each do |reportsortby| %>
      <tr>
        <td><%= reportsortby.id %></td>
        <td><%= link_to reportsortby.fieldname, reportsortby_path(reportsortby) %></td>
        <td><%= reportsortby.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_reportsortby_path(reportsortby), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', reportsortby_path(reportsortby), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_reportsortby_path, :class => 'btn btn-primary' %>
