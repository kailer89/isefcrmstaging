<h1>Finalidad Cultivos</h1>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Finalidad cultivo</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @finalidad_cultivos.each do |finalidad_cultivo| %>
      <tr>
        <td><%= finalidad_cultivo.id %></td>
        <td><%= link_to finalidad_cultivo.finalidad_cultivo, finalidad_cultivo_path(finalidad_cultivo) %></td>
        <td><%= finalidad_cultivo.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_finalidad_cultivo_path(finalidad_cultivo), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', finalidad_cultivo_path(finalidad_cultivo), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_finalidad_cultivo_path, :class => 'btn btn-primary' %>
