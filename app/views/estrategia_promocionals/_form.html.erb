<%= form_for @estrategia_promocional, :html => { :class => 'form-horizontal' } do |f| %>
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to 'Cancel', estrategia_promocionals_path, :class => 'btn' %>
<%=render :partial=>"shared/breadcrumbs", :locals=>{:model_name=>request.path_parameters[:controller],:model_id=>@estrategia_promocional.id, :parent_model=>nil, :parent_id=>nil}%>

  <fieldset>
   <div class="well"> <legend><%= controller.action_name.capitalize %> Estrategia Promocional</legend>
   
       <div class="control-group">
         <%= f.label :status_de_la_estrategia, :class => 'control-label' %>
         <div class="controls">
           <%= f.text_field :status_de_la_estrategia, :class => 'text_field' %>
         </div>
       </div>
   
       <div class="control-group">
         <%= f.label :fecha_para_llevarla_a_cabo, :class => 'control-label' %>
         <div class="controls">
           <%= f.date_select :fecha_para_llevarla_a_cabo, :class => 'date_select' %>
         </div>
       </div>
   
       <div class="control-group">
         <%= f.label :reponsable_para_llevarla_a_cabo, :class => 'control-label' %>
         <div class="controls">
           <%= f.text_field :reponsable_para_llevarla_a_cabo, :class => 'text_field' %>
         </div>
       </div>
   
       <div class="control-group">
         <%= f.label :descripcion_detallada, :class => 'control-label' %>
         <div class="controls">
           <%= f.text_area :descripcion_detallada, :class => 'text_area' %>
         </div>
       </div>
   
       <div class="control-group">
         <%= f.label :mercado_meta_id, :class => 'control-label' %>
         <div class="controls">
           <%= f.number_field :mercado_meta_id, :class => 'number_field' %>
         </div>
       </div>
   
       <div class="control-group">
         <%= f.label :documento_path, :class => 'control-label' %>
         <div class="controls">
           <%= f.text_field :documento_path, :class => 'text_field' %>
         </div>
       </div></div>
<%=render :partial=>"shared/tareas", :locals=>{:model_id=>@estrategia_promocional.id}%>
<%=render :partial=>"shared/memos", :locals=>{:model_id=>@estrategia_promocional.id}%>
<%=render :partial=>"shared/attachments", :locals=>{:model_id=>@estrategia_promocional.id}%> 
<%=render :partial=>"shared/llamadas", :locals=>{:model_id=>@estrategia_promocional.id}%> 
    <div class="form-actions">
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to 'Cancel', estrategia_promocionals_path, :class => 'btn' %>
    </div>
  </fieldset>
<% end %>
