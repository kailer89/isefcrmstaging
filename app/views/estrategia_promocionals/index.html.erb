<h1>Estrategia Promocionals</h1>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Status de la estrategia</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @estrategia_promocionals.each do |estrategia_promocional| %>
      <tr>
        <td><%= estrategia_promocional.id %></td>
        <td><%= link_to estrategia_promocional.status_de_la_estrategia, estrategia_promocional_path(estrategia_promocional) %></td>
        <td><%= estrategia_promocional.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_estrategia_promocional_path(estrategia_promocional), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', estrategia_promocional_path(estrategia_promocional), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_estrategia_promocional_path, :class => 'btn btn-primary' %>
